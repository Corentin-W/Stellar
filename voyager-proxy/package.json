{
  "name": "stellar-voyager-proxy",
  "version": "1.0.0",
  "description": "Proxy server for Voyager Application Server integration with Stellar platform",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "node --test",
    "lint": "eslint src/**/*.js",
    "docker:build": "docker build -t stellar-voyager-proxy .",
    "docker:run": "docker run -p 3000:3000 --env-file .env stellar-voyager-proxy"
  },
  "keywords": [
    "voyager",
    "astronomy",
    "telescope",
    "proxy",
    "robotarget"
  ],
  "author": "Stellar Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "uuid": "^9.0.1",
    "joi": "^17.11.0",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "eslint": "^8.56.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
